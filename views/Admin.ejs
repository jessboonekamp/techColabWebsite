<body>
    <div class="container">
        <div class="row">
            <nav class="navbar navbar-expand-md navbar-light bg-light">
                <a class="navbar-brand" href="#">Logo</a>
                <button class="navbar"></button>
            </nav>
        </div>
        <div class="row">
            <div class="col"></div>
            <div class="col-8">
                <h1 class="display-1">Admin Page</h1>
                <% if(Content == "Admin") { %>
                    <%- include('partials/Admin/Admin.ejs'); %>
                <% }else if(Content == "AddStudent"){ %>
                    <%- include('partials/Admin/AddStudent.ejs'); %>
                <% }else if(Content == "Students"){ %>
                    <%- include('partials/Admin/Students.ejs'); %>
                <% }else if(Content == "AddProject"){ %>
                    <%- include('partials/Admin/AddProject.ejs'); %>
                <% }else if(Content == "Projects"){ %>
                    <%- include('partials/Admin/Projects.ejs'); %>
                <% }else{ %>
                    <%- include('partials/Error.ejs'); %>
                <% } %>
                
            </div>
            <div class="col"></div>
        </div>
    </div>
    
</body>
