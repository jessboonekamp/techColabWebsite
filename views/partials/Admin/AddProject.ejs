<div>
    <h1 class="display-4">Add a project</h1>
</div>

<div class="addProjectForm">
    <form method="POST">
        <div>
            <label for="title">Project Title</label>
            <input type="text" name="title" id="">
        </div>
        <div>
            <label for="project_type">Project Type</label>
            <select name="project_type" id="">
                <option value="capstone">Capstone</option>
                <option value="techcolab">TechColab</option>
            </select>
        </div>
        <div>
            <label for="project_date">Project Date</label>
            <input type="date" name="project_date" id="">
        </div>
        <div>
            <label for="biography">Biography</label>
            <!-- <input type="text" name="biography" id=""> -->
            <textarea name="biography" id="" cols="30" rows="10"></textarea>
        </div>
        <%- include('../FileUpload') %>
        <input type="submit" name="submit" id="">
    </form>
</div>